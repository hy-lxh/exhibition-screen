{
  "name": "exhibtion-hall-visualization",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": ".",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "eslint": "eslint './src/**/*.{ts,tsx}' --fix",
    "prettier": "prettier --write './src/**/*.{less,ts,tsx}'",
    "lint-staged": "lint-staged",
    "commit": "commitlint -E HUSKY_GIT_PARAMS"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{ts,tsx,less}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "animejs": "^3.2.1",
    "axios": "^1.4.0",
    "echarts": "^5.4.2",
    "less": "^4.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@types/animejs": "^3.1.7",
    "@types/echarts": "^4.9.17",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^20.1.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.23",
    "postcss-preset-env": "^8.3.2",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.2",
    "vite": "^4.3.8",
    "vite-plugin-mock-dev-server": "^1.1.13"
  }
}
